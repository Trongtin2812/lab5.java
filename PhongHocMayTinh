class PhongHocMayTinh extends PhongHoc {
    private int soLuongMayTinh;

    public PhongHocMayTinh(String maPhong, String dayNha, double dienTich, int soLuongBongDen, int soLuongMayTinh) {
        super(maPhong, dayNha, dienTich, soLuongBongDen);
        this.soLuongMayTinh = soLuongMayTinh;
    }

    public int getSoLuongMayTinh() {
        return soLuongMayTinh;
    }

    public void setSoLuongMayTinh(int soLuongMayTinh) {
        this.soLuongMayTinh = soLuongMayTinh;
    }

    @Override
    public String toString() {
        return "PhongHocMayTinh{" +
                "maPhong='" + getMaPhong() + '\'' +
                ", dayNha='" + getDayNha() + '\'' +
                ", dienTich=" + getDienTich() +
                ", soLuongBongDen=" + getSoLuongBongDen() +
                ", soLuongMayTinh=" + soLuongMayTinh +
                '}';
    }
}
