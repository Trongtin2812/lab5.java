class PhongHoc {
    private final String maPhong;
    private final String dayNha;
    private final double dienTich;
    private final int soLuongBongDen;

    public PhongHoc(String maPhong, String dayNha, double dienTich, int soLuongBongDen) {
        this.maPhong = maPhong;
        this.dayNha = dayNha;
        this.dienTich = dienTich;
        this.soLuongBongDen = soLuongBongDen;
    }

    public String getMaPhong() {
        return maPhong;
    }

    public String getDayNha() {
        return dayNha;
    }

    public double getDienTich() {
        return dienTich;
    }

    public int getSoLuongBongDen() {
        return soLuongBongDen;
    }

    @Override
    public String toString() {
        return "PhongHoc{" +
                "maPhong='" + maPhong + '\'' +
                ", dayNha='" + dayNha + '\'' +
                ", dienTich=" + dienTich +
                ", soLuongBongDen=" + soLuongBongDen +
                '}';
    }
}
