class PhongHocThiNghiem extends PhongHoc {
    private final String chuyenNganh;
    private final int sucChua;
    private final boolean coBonRua;

    public PhongHocThiNghiem(String maPhong, String dayNha, double dienTich, int soLuongBongDen, String chuyenNganh,
                             int sucChua, boolean coBonRua) {
        super(maPhong, dayNha, dienTich, soLuongBongDen);
        this.chuyenNganh = chuyenNganh;
        this.sucChua = sucChua;
        this.coBonRua = coBonRua;
    }

    public boolean isCoBonRua() {
        return coBonRua;
    }

    @Override
    public String toString() {
        return "PhongHocThiNghiem{" +
                "maPhong='" + getMaPhong() + '\'' +
                ", dayNha='" + getDayNha() + '\'' +
                ", dienTich=" + getDienTich() +
                ", soLuongBongDen=" + getSoLuongBongDen() +
                ", chuyenNganh='" + chuyenNganh + '\'' +
                ", sucChua=" + sucChua +
                ", coBonRua=" + coBonRua +
                '}';
    }
}
